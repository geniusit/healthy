{
  "name": "react-healthy",
  "version": "0.1.1",
  "author": "Katia Wheeler",
  "description": "Incrementally check and alert your users to the health of your APIs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/katiawheeler/healthy/issues"
  },
  "homepage": "https://github.com/katiawheeler/healthy#readme",
  "main": "build/index.js",
  "types": "lib/index.d.ts",
  "module": "build/index.js",
  "files": [
    "build",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/katiawheeler/healthy.git"
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "tsc",
    "test": "react-scripts test --watch --collectCoverage",
    "test:publish": "react-scripts test -u",
    "test:coverage": "react-scripts test --collectCoverage",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "tslint --fix -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test:publish && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@types/enzyme": "^3.1.16",
    "@types/jest": "23.3.13",
    "@types/node": "10.12.21",
    "@types/react": "16.7.22",
    "@types/react-dom": "16.0.11",
    "babel-core": "7.0.0-bridge.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0",
    "react-scripts": "^2.1.3",
    "react-test-renderer": "^16.7.0",
    "source-map-explorer": "^1.7.0",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/components/index.ts",
      "!src/**/*.styles.js",
      "!src/serviceWorker.ts",
      "!src/index.tsx"
    ]
  }
}
